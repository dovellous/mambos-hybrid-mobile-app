{
  "name": "mambos-chicken",
  "private": true,
  "version": "1.0.0",
  "description": "Mambo's Chicken",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev && npm run includes-scripts",
    "android": "npx cap sync && npm run build-capacitor-android && npx cap run android",
    "dev": "cross-env NODE_ENV=development vite && npm run includes-scripts",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy ./node_modules/material-icons/iconfont/*.* ./src/fonts/ && npm run includes-scripts",
    "includes-scripts": "cpy ./src/js/frameworks/f7/styles/themes/*.* ./www/css/themes/ && cpy ./src/js/frameworks/f7/styles/themes/*.* ./public/css/themes/ "
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^3.0.0",
    "@capacitor-community/fcm": "^2.0.2",
    "@capacitor-community/sqlite": "^4.0.1",
    "@capacitor-community/text-to-speech": "^2.0.1",
    "@capacitor/action-sheet": "^4.0.1",
    "@capacitor/android": "^4.1.0",
    "@capacitor/app": "^4.0.1",
    "@capacitor/app-launcher": "^4.0.1",
    "@capacitor/browser": "^4.0.1",
    "@capacitor/camera": "^4.1.0",
    "@capacitor/clipboard": "^4.0.1",
    "@capacitor/core": "^4.1.0",
    "@capacitor/device": "^4.0.1",
    "@capacitor/dialog": "^4.0.1",
    "@capacitor/filesystem": "^4.1.0",
    "@capacitor/geolocation": "^4.0.1",
    "@capacitor/haptics": "^4.0.1",
    "@capacitor/ios": "^4.1.0",
    "@capacitor/keyboard": "^4.0.1",
    "@capacitor/local-notifications": "^4.0.1",
    "@capacitor/motion": "^4.0.1",
    "@capacitor/network": "^4.0.1",
    "@capacitor/push-notifications": "^4.0.1",
    "@capacitor/screen-reader": "^4.0.1",
    "@capacitor/share": "^4.0.1",
    "@capacitor/splash-screen": "^4.0.1",
    "@capacitor/status-bar": "^4.0.1",
    "@capacitor/storage": "^1.2.3",
    "@capacitor/text-zoom": "^4.0.1",
    "@capacitor/toast": "^4.0.1",
    "@robingenz/capacitor-app-update": "^1.1.0",
    "@robingenz/capacitor-badge": "^1.2.0",
    "@robingenz/capacitor-firebase-authentication": "^0.3.11",
    "cap-firebase-dynamic-links": "^0.3.1",
    "capacitor-native-biometric": "^4.1.3",
    "capacitor-native-settings": "^4.0.0",
    "capacitor-plugin-filedownload": "^1.0.3",
    "capacitor-rate-app": "^3.0.0",
    "capacitor-secure-storage-plugin": "^0.8.0",
    "crypto-js": "^4.1.1",
    "dom7": "^4.0.4",
    "dovellous-jsnippets": "^1.0.2",
    "firebase": "^9.9.4",
    "firebaseui": "^6.0.1",
    "framework7": "^7.0.7",
    "framework7-icons": "^5.0.5",
    "jquery": "^3.6.1",
