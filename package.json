    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.1.0",
    "@lsqswl/rsaencrypt": "^1.0.3",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^7.8.1",
    "rollup-plugin-framework7": "^1.2.1",
    "sass": "^1.54.8",
    "vite": "^3.1.0",
    "workbox-cli": "^6.5.4"
  }
}
